{
  "name": "rgl",
  "version": "0.1.3",
  "description": "An engine for terminal games",
  "keywords": [
    "rgl,colors,ansi,terminal,tty,map,game"
  ],
  "homepage": "https://github.com/Valen-H/RGL3#README.md",
  "bugs": {
    "url": "https://github.com/Valen-H/RGL3/issues",
    "email": "alternativexxxy@gmail.com"
  },
  "license": "ISC",
  "author": "V. H. <alternativexxxy@gmail.com> (https://github.com/Valen-H)",
  "contributors": [],
  "files": [
    "*"
  ],
  "main": "./dist/lib/rgl.js",
  "browser": false,
  "man": [
    "doc"
  ],
  "directories": {
    "lib": "./lib",
    "bin": "./bin",
    "man": "./man",
    "doc": "./doc",
    "example": "./test",
    "test": "./test"
  },
  "typings": "./lib/typings/",
  "typesVersions": {
    ">=3.0": {
      "*": [
        "./lib/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Valen-H/RGL3.git",
    "directory": "./"
  },
  "config": {
    "port": 8080
  },
  "engines": {
    "node": ">=13.0",
    "npm": ">=4.0"
  },
  "engineStrict": true,
  "os": [],
  "cpu": [],
  "bin": {
    "rgl": "./dist/bin/rgl.js"
  },
  "preferGlobal": true,
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "node index.js",
    "test": "set NODE_DEBUG=RGL&node test/index.js",
    "debug": "set NODE_DEBUG=RGL&node --expose-gc --inspect test/index.js",
    "bat": "./start.bat",
    "build": "tsc -w",
    "doc": "jsdoc ./dist -r -c jsdoc.json -d ./doc -R README.md -P package.json",
    "prepack": "rm rgl-*.tgz"
  },
  "bundleDependencies": [
    "chalk",
    "fs-extra",
    "tslib",
    "command-line-args"
  ],
  "dependencies": {
    "chalk": "^4.1.1",
    "command-line-args": "^5.1.1",
    "fs-extra": "^10.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^16.0.0",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {},
  "optionalDependencies": {
    "strip-ansi": "^7.0.0"
  }
}
